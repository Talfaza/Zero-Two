options {
    STATIC = false;
}


TOKEN: {
    <NUMBER: ["0"-"9"]+> |
    <SYMBOL: [\+\-\*/\^\(\)]> |
    <WHITESPACE: (" " | "\t" | "\r" | "\n")+ >
}

SKIP: {
    <WHITESPACE>
}

void parse() :
        {}
{
    Calculat() <EOF>
}

void Calculat() :
        {}
{
    (Number() Symbol() Number())+
}

void Number() :
        {}
{
    <NUMBER>
}

void Symbol() :
        {}
{
    <SYMBOL>
}
